				set {_p}'s health to {_h}
		wait for 0.2 seconds
		teleport {_p} to {_p}
	if {ability::%{_p}'s uuid%} is "Kim Jong Un":
		set {RRLcd::%{_p}'s uuid%} to 15 # COOLDOWN
