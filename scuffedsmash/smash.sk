on left click:
	if player's held item is iron ingot of mending named "&7Down Attack":
		loop all entities in radius 2 of clicked block:
			{hitcd::%loop-entity%} is false
			loop-entity is not player
			set {percent::%loop-entity%} to {percent::%loop-entity%} + random integer between 2 and 5
			set {launch::%loop-entity%} to {percent::%loop-entity%} * 1.5 / 100
			play sound "entity.generic.hurt" at volume 1 and pitch 1 for player and loop-entity
			push loop-entity downwards at speed 0.25 + {launch::%loop-entity%}
			set {hitcd::%loop-entity%} to false
			wait for 0.65 second
			set {hitcd::%loop-entity%} to true

on right click:
	player's held item is player's skull
	cancel event
	send "&f&l%player%'s Stats"
	send "&eKOs: &7%{kills::%player%}%"
	send "&eDeaths: &7%{deaths::%player%}%"
	send "&eJumps: &7%{jumps::%player%}%"
	send "&eCoins: &7%{coins::%player%}%"

on right click on entity:
	if player's held item is air:
		send "&c&l%{percent::%clicked entity%}%%%" to player

on jump:
	set {jumps::%player%} to {jumps::%player%} + 1

on flight toggle:
	if player's gamemode is not creative:
		cancel event
		wait a tick 
		set {jumps::%player%} to {jumps::%player%} + 1
		teleport player to player's location
		set player's flight state to false
		if {character::%player%} is "Gingerbrave":
			push player upwards at speed 1.3
			push player forwards at speed 0.55
			stop
		if {character::%player%} is "Dug":
			push player forwards at speed 0.4
			push player upwards at speed 1
			stop
		if {character::%player%} is "Perry":
			push player upwards at speed 1.3
			push player forwards at speed 0.45
			stop
		if {character::%player%} is "Sans":
			push player upwards at speed 1.15
			push player forwards at speed 0.5
			stop
		if {character::%player%} is "Gumball":
			push player upwards at speed 1.18
			push player forwards at speed 0.35
			stop
		if {character::%player%} is "Red":
			push player upwards at speed 1.05
			push player forwards at speed 0.5
			stop
		if {character::%player%} is "Peter":
			push player upwards at speed 1
			push player forwards at speed 0.45
			stop
		if {character::%player%} is "Pink Bean":
			push player upwards at speed 1.05
			push player forwards at speed 0.4
			stop
		if {character::%player%} is "Walter":
			push player upwards at speed 1.2
			push player forwards at speed 0.35
			stop
		if {character::%player%} is "Gwen":
			push player upwards at speed 1.35
			push player forwards at speed 0.5
			stop
		if {character::%player%} is "Mordecai":
			push player upwards at speed 1.35
			push player forwards at speed 0.2
			stop
		if {character::%player%} is "Jett":
			push player upwards at speed 1.14
			push player forwards at speed 0.3
			stop
		if {character::%player%} is "Mabel":
			push player upwards at speed 1.2
			push player forwards at speed 0.35
			stop
		else:
			push player upwards at speed 1.15
			push player forwards at speed 0.45

on explode:
	cancel event
	loop all entities within radius 6 of event-entity:
		set {percent::%loop-entity%} to {percent::%loop-entity%} + 20
		push loop-entity backwards at speed 1.5 + {percent::%loop-entity%} * 2 / 100
		play sound "entity.generic.explode" at volume 1 and pitch 1 for loop-entity
		show large explosion at location of event-entity

on chat:
	set chat format to "&f[&e%{coins::%player%}%⛁&f] %{tag::%player%}% &f%player%: &7%message%"
	play sound "%{chatsound::%player%}%" at volume 1 and pitch 2 for all players

on first join:
	broadcast "&a&l%player% just joined for the 1st time!"
	send "&fSally: &7Welcome, %player%!"
	send "&fSally: &7Right click the guide in your inventory to learn how to play."
	set {coins::%player%} to {coins::%player%} + 400
	send "&aThanks for playing early!"
	send "&eYou have received &6400 coins"
	wait 1 second
	set line 4 of player's scoreboard to colored "&eCoins: &r%{coins::%player%}%"

command /exit:
	trigger:
		if {smash::%player%} is not "smash":
			clear {smash::%player%}
			teleport player to location 0.5, 66, 0.5 in world "smash"
			clear player's inventory
			set slot 3 of player's inventory to emerald of mending named "&bFighter List"
			set slot 0 of player's inventory to player's skull named "&aStats"
			set slot 8 of player's inventory to chest named "&6Cosmetics"
			set slot 4 of player's inventory to clock of mending named "&cSm&4ash"
			set slot 5 of player's inventory to book of mending named "&eGuide"
			clear {game}

command /msg <offline player> <text>:
	aliases: /w
	usage: &fUsage: &7/msg <online-player> <message> 
	trigger:
		send "&7&lYou &d&l--> &7&l%arg-1%: &f%arg-2%" to player
		send "&7&l%player% &d&l--> &7&lYou: &f%arg-2%" to arg-1

command /emergencyquit:
	permission: op
	trigger:
		clear {game}
on quit:
	if {smash::%player%} is "smash":
		set {queue} to {queue} - 1
		set {stocks::%player%} to 0
		broadcast "&e%player% &crage quit"
		if {queue} is 1:
			loop all players:
				if {smash::%loop-player%} is "smash":
					teleport loop-player to location 0.5, 66, 0.5 in world "smash"
					clear loop-player's inventory
					set slot 3 of loop-player's inventory to emerald of mending named "&bFighter List"
					set slot 0 of loop-player's inventory to loop-player's skull named "&aStats"
					set slot 8 of loop-player's inventory to chest named "&6Cosmetics"
					set slot 4 of loop-player's inventory to clock of mending named "&cSm&4ash"
					play sound "entity.player.levelup" at volume 1 and pitch 1 for all players

