on inventory click:
	if name of event-inventory is "&eSelect Gamemode":
		cancel event
		if index of event-slot = 11:
			{smash::%player%} is not "smash"
			set {smash::%player%} to "zombie"
			set {deathloc::%player%} to location 0.5, 77, -19.5 in world "smash"
			set {spawnloc::%player%} to location 0.5, 78, -23 in world "smash"
			teleport player to {spawnloc::%player%}
			set {stocks::%player%} to 3
			set {percent::%player%} to 0
			make player execute command "/percent"
			clear player's inventory
			give 1 of gold ingot of mending named "&bNeutral Special &3(Right click)" to player
			give 1 of gold ingot of mending named "&eSide Special &3(Right click)" to player
			give 1 of gold ingot of mending named "&aUp Special &3(Right click)" to player
			give 1 of gold ingot of mending named "&dDown Special &3(Right click)" to player
			give 1 of iron ingot of mending named "&7Up Attack" to player
			give 1 of iron ingot of mending named "&7Down Attack" to player
			apply slow falling to player for 3 seconds
		if index of event-slot = 15:
			{smash::%player%} is not "smash"
			set {smash::%player%} to "training"
			set {deathloc::%player%} to location 0.5, 78, 90.5 in world "smash"
			set {spawnloc::%player%} to location 0.5, 78, 95 in world "smash"
			teleport player to {spawnloc::%player%}
			set {stocks::%player%} to 10
			set {percent::%player%} to 0
			make player execute command "/percent"
			clear player's inventory
			give 1 of gold ingot of mending named "&bNeutral Special &3(Right click)" to player
			give 1 of gold ingot of mending named "&eSide Special &3(Right click)" to player
			give 1 of gold ingot of mending named "&aUp Special &3(Right click)" to player
			give 1 of gold ingot of mending named "&dDown Special &3(Right click)" to player
			give 1 of iron ingot of mending named "&7Up Attack" to player
			give 1 of iron ingot of mending named "&7Down Attack" to player
			give 1 of emerald of mending named "&bFighter List" to player
			apply slow falling to player for 3 seconds
		if index of event-slot = 13:
			if {game} is true:
				send "&c&lWhoops! &eLooks like there is a battle happening currently. Please wait until it has finished."
				stop
			if {smash::%player%} is "smash":
				make player execute command "/leavequeue"
				stop
			set {smash::%player%} to "smash"
			set {stocks::%player%} to 3
			set {spawnloc::%player%} to location 0.5, 85, 25 in world "smash"
			set {deathloc::%player%} to location 0.5, 79, 7.5 in world "smash"
			set {queue} to {queue} + 1
			set {percent::%player%} to 0
			make player execute command "/percent"
			broadcast "&a%player% has joined the queue"
			broadcast "&ePlayers found &a%{queue}%/8"
			if {queue} is smaller than 2:
				broadcast "&7Waiting for more players..."
			if {queue} is greater than 1:
				{battlestart} is not true
				set {battlestart} to true
				{battlestart} is true
				set {_e} to 15
				loop 15 times:
					broadcast "&aBattle starting in %{_e}% seconds"
					set {_e} to {_e} - 1
					wait for 1 second
					if {queue} is less than 2:
						clear {battlestart}
						stop
				clear {battlestart}
				set {game} to true
				loop all players:
					if {smash::%loop-player%} is "smash":
						set {percent::%loop-player%} to 0
						teleport loop-player to {spawnloc::%loop-player%}
						apply slow falling to loop-player for 3 seconds
						clear loop-player's inventory
						give 1 of gold ingot of mending named "&bNeutral Special &3(Right click)" to loop-player
						give 1 of gold ingot of mending named "&eSide Special &3(Right click)" to loop-player
						give 1 of gold ingot of mending named "&aUp Special &3(Right click)" to loop-player
						give 1 of gold ingot of mending named "&dDown Special &3(Right click)" to loop-player
						give 1 of iron ingot of mending named "&7Up Attack" to loop-player
						give 1 of iron ingot of mending named "&7Down Attack" to loop-player
						loop 100 times:
							set {cd::%loop-player%} to true
				
command /leavequeue:
	trigger:
		if {smash::%player%} is "smash":
			set {queue} to {queue} - 1
			clear {smash::%player%}
			broadcast "&c%player% has left the queue"
			broadcast "&ePlayers found &a%{queue}%/8"

