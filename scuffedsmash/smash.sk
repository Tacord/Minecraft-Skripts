on player walk:
	if block below event-entity is not air or yellow wool:
		set player's flight state to true
		set {up::%player%} to true
		set {airdodge::%player%} to true

every 2 seconds:
	loop all entities:
		set {hitcd::%loop-entity%} to true
		set title of loop-entity's scoreboard to "&a&lScuffed&e&lSmash"
		set line 3 of loop-entity's scoreboard to colored "&cDamage: &r%{percent::%loop-entity%}%%%"
		set line 4 of loop-entity's scoreboard to colored "&eCoins: &r%{coins::%loop-entity%}%"
		set line 5 of loop-entity's scoreboard to colored "&bStocks Left: &r%{stocks::%loop-entity%}%"
		set line 6 of loop-entity's scoreboard to colored "&dCharacter: &r%{character::%loop-entity%}%"
	loop all zombies:
		if block below loop-entity is air:
			teleport loop-entity to loop-entity
			push loop-entity upwards at speed 1

on combust:
	cancel event

on damage:
	victim is player or zombie or villager
	cancel event
	{resist::%victim%} is not true
	if damage cause is not fall:
		{counter::%victim%} is not true
		if {hitcd::%victim%} is true:
			loop 10 times:
				show critical hit at block 2 above victim
			set {_e} to random integer between 1 and 2
			if {_e} is 1:
				if victim is zombie:
					set {coins::%attacker%} to {coins::%attacker%} + 1
					play sound "entity.player.levelup" at volume 0.4 and pitch 3 for attacker
			set {launch::%victim%} to {percent::%victim%} * 1.5 / 100
			set {percent::%victim%} to {percent::%victim%} + random integer between 2 and 9
			set {launch::%victim%} to {percent::%victim%} * 1.5 / 100
			if {konami::%victim%} is true:
				set {percent::%victim%} to {percent::%victim%} + 3
				set {launch::%victim%} to {percent::%victim%} / 100
			send action bar "&e&l%{percent::%victim%}%%%" to attacker
			make victim execute command "/hurtpercent"
			if attacker's held item is iron ingot of mending named "&7Up Attack":
				play sound "entity.generic.hurt" at volume 1 and pitch 1 for victim and attacker
				push victim upwards at speed 0.6 + {launch::%victim%}
				set {hitcd::%victim%} to false
				wait for 0.65 second
				set {hitcd::%victim%} to true
				stop
			if attacker's held item is iron ingot of mending named "&7Down Attack":
				play sound "entity.generic.hurt" at volume 1 and pitch 1 for victim and attacker
				push victim downwards at speed 0.25 + {launch::%victim%}
				set {hitcd::%victim%} to false
				wait for 0.65 second
				set {hitcd::%victim%} to true
				stop
			if attacker's held item is iron sword of mending named "&fPerry's Dagger" or "&fPlayco Armboy":
				set {percent::%victim%} to {percent::%victim%} + random integer between 8 and 12
				play sound "entity.player.attack.crit" at volume 1 and pitch 1.2 for victim and attacker
				push victim in direction of attacker at speed 1.25 + {launch::%victim%}
				push victim upwards at speed 0.6 + {launch::%victim%} / 2
				set {hitcd::%victim%} to false
				wait for 0.5 second
				set {hitcd::%victim%} to true
				stop
			else:
				play sound "entity.generic.hurt" at volume 1 and pitch 1 for victim and attacker
				push victim in direction of attacker at speed 1 + {launch::%victim%}
				push victim upwards at speed 0.5 + {launch::%victim%} / 2
				set {hitcd::%victim%} to false
				wait for 0.65 second
				set {hitcd::%victim%} to true
				stop
		else:
			play sound "entity.generic.small_fall" at volume 1 and pitch 3 for victim and attacker

